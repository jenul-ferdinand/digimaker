{
  "name": "@digimakers/core",
  "version": "0.3.16",
  "description": "Core library for Digimaker - docx to PDF conversion",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js"
    }
  },
  "scripts": {
    "build": "tsc && node scripts/copy-docling.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.10",
    "@digimakers/docling-cleaner": "^1.2.16",
    "ai": "6.0.39",
    "dotenv": "^17.2.3",
    "express": "5.2.1",
    "mammoth": "^1.8.0",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "puppeteer": "24.35.0",
    "word-extractor": "^1.0.4",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@types/express": "5.0.6",
    "@types/node": "24.0.0",
    "typescript": "^5.5.0"
  },
  "files": [
    "dist/**/*",
    "dist/docling-cleaner/**/*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenul-ferdinand/digimaker.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/jenul-ferdinand/digimaker/issues"
  },
  "homepage": "https://github.com/jenul-ferdinand/digimaker#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
